{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/**/*.ts"
  ],
  "projectDocuments": [
    "examples/**/*.md"
  ],
  "out": "docs",
  "gitRevision": "master",
  "excludeInternal": true,
  "cleanOutputDir": true,
  "groupOrder": [
    "Documents",
    "Namespaces",
    "Modules",
    "*"
  ],
  "categoryOrder": [
    "Examples",
    "*"
  ]
}